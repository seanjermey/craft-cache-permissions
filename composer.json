{
    "name": "seanjermey/craft-cache-permissions",
    "description": "Plugin allows you to restrict who can clear each cache group.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "cache permissions"
    ],
    "support": {
        "docs": "https://github.com/seanjermey/craft-cache-permissions/blob/master/README.md",
        "issues": "https://github.com/seanjermey/craft-cache-permissions/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sean Jermey",
            "homepage": "https://github.com/seanjermey"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "seanjermey\\cachepermissions\\": "src/"
        }
    },
    "extra": {
        "name": "Cache Permissions",
        "handle": "cache-permissions",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/seanjermey/craft-cache-permissions/master/CHANGELOG.md",
        "class": "seanjermey\\cachepermissions\\CachePermissions"
    }
}
